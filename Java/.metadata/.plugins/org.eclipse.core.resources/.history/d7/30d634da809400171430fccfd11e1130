package memo2;

public class View {

	//입력창
	public void intro() {
		println("------------명령어를 입력하세요------------");
        println("c : 쓰기, r : 읽기, u : 수정, d : 삭제, l : 목록");
        println("exit : 종료");
        println("-------------------------------------");
	}
	
	public void println(String str) {
		System.out.println(str);
	}
	
	public void print(String str) {
		System.out.print(str);
	}
}
}
