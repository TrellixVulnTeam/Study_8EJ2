	public void main(String args) {
		//calc (10000, 3750)
		int money = 10000;
		int price = 3750;
	}
	
	public void calc(int money, int price) {
		//총 거스름돈
		System.out.println(a-b);
		//오천원
		System.out.println(a/500);
		//천원
		System.out.println();
		//오백원
